{
  "manifest_version": 2,
  "name": "Lil' BIT less",
  "version": "1.0",
  "minimum_chrome_version": "28",
  "permissions": [ "<all_urls>", "storage","alarms","notifications"],
  
  "description": "Checks and Monitors your shopping list with updated bitcoin values. Pay for a little BIT less.",
   "background": {
    "scripts": ["jquery.js","background.js"]
  },
  
  "icons": { 
    "16": "icon16x16.png",
        "48": "icon48x48.png",
    "128": "icon128x128.png" },

  "browser_action": {
    "default_popup": "money.html",
    "default_icon": "icon19x19.png"
  }

}